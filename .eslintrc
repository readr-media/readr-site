{
  "extends": [ "plugin:vue/base" ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "plugins": [
      "import"
  ],
  "rules": { 
    "array-bracket-spacing": [ 2, "always" ],
    "camelcase": [ 0, "" ],
    "yoda": [ 0, "never" ]
  },
  "globals": {
    "FB": true,
    "FormData": true,
    "expect": true,
    "gapi": true,
    "location": true,
    "source": true
  },
  "env": {
    "mocha": true
  }
}