{
  "name": "readr-site",
  "description": "A Vue.js project",
  "author": "MirrorMedia <master@mirrormedia.mg>",
  "private": true,
  "scripts": {
    "api": "nyc --reporter=lcov --reporter=text-lcov mocha --timeout 10000 --reporter list test/api/specs/*.spec.js --exit",
    "dev": "forever -e err.log serverCluster.js",
    "start": "cross-env NODE_ENV=production PORT=80 forever -e err.log serverCluster.js",
    "build": "rimraf dist && yarn run build:client && yarn run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "e2e": "node test/e2e/runner.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "validate": "npm ls"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.0",
    "cookies": "^0.7.1",
    "cross-env": "^5.0.5",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "humps": "^2.0.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "moment": "^2.19.3",
    "redis-connection-pool": "^1.6.1",
    "request-ip": "^2.0.2",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.4",
    "superagent": "^3.8.1",
    "uuid": "^3.1.0",
    "validator": "^9.1.2",
    "vue": "^2.4.4",
    "vue-cookie": "^1.1.4",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.4",
    "vuex": "^2.4.0",
    "vuex-router-sync": "^4.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chokidar": "^1.7.0",
    "chromedriver": "^2.33.2",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-enough": "^0.2.8",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^2.1.0",
    "faker": "^4.1.0",
    "file-loader": "^0.11.2",
    "forever": "^0.15.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "karma": "^1.7.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.6",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.16",
    "nyc": "^11.3.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.2",
    "selenium-server": "^3.7.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "supertest": "^3.0.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "pre-commit": [
    "lint"
  ],
  "nyc": {
    "exclude": [
      "api/config.js"
    ],
    "include": [
      "api/**/*.js"
    ]
  }
}
