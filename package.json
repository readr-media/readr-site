{
  "name": "readr-site",
  "description": "A Vue.js project",
  "author": "MirrorMedia <master@mirrormedia.mg>",
  "private": true,
  "scripts": {
    "api": "nyc --reporter=lcov --reporter=text-lcov mocha --timeout 10000 --reporter list test/api/specs/posts.spec.js --exit",
    "dev": "yarn run npm-foreve",
    "start": "cross-env NODE_ENV=production PORT=80 yarn run npm-forever",
    "build": "rimraf dist && yarn run build:client && yarn run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "e2e": "node test/e2e/runner.js",
    "lint": "eslint --ext .js,.vue src api --max-warnings 0",
    "lintfix": "eslint --ext .js,.vue --ignore-path .gitignore src api test/unit/specs --max-warnings 0 --fix",
    "npm-forever": "forever -o out.log -e err.log -c 'node --expose-gc' serverCluster.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "validate": "npm ls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --max-warnings 0"
    ]
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.4",
