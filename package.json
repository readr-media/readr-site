    "joi": "^13.1.2",
    "jsdom": "^11.7.0",
    "jsonwebtoken": "^8.1.0",
    "kc-scroll": "^0.2.71",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "memwatch-next": "^0.3.0",
    "moment": "^2.21.0",
    "mongodb": "^3.0.1",
    "multer": "^1.3.0",
    "open-graph-scraper": "^3.1.5",
    "qs": "^6.5.1",
    "redis-connection-pool": "^1.6.1",
